{
    "name": "tarioch/phealbundle",
    "description": "Adds pheal support to your Symfony2 project",
    "keywords": ["Pheal", "PhealNG", "EVE Online", "EVE API"],
    "homepage": "https://github.com/tarioch/phealbundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
		"name": "Talos Katuma",
		"homepage": "https://hplcorp.net"
        }
    ],
    "support": {
	"issues": "https://github.com/tarioch/phealbundle/issues"
    },
    "require": {
        "php": ">=5.4.4",
	"symfony/framework-bundle": ">=2.3",
	"jms/di-extra-bundle" : "1.4.*",
	"3rdpartyeve/phealng" : "dev-master"
    },
    "require-dev": {
	"phpunit/phpunit" : "3.7.*",
	"mockery/mockery" : "dev-master@dev"
    },
    "autoload": {
	"psr-0": { "Tario\\PhealBundle": "" }
    },
    "target-dir": "Tario/PhealBundle"
}
